doctype
html(lang="en")
	head
		meta(charset="UTF-8")
		title codigo
	body
		h1 codigo 
		- var colores = ['purple', 'aqua', 'teal', 'lime']
		ul
			each color in colores
				li(style={color: color})=color

		table(border=1)
			- for(var i = 1; i <= 10; i++)
			tr
				- for(var j = 1; j<=20; i++)
					td f#{i} c#{j} 